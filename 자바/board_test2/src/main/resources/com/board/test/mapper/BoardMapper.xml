<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.board.test.mapper.BoardMapper">

	<select id="boardList" resultType="com.board.test.domain.Board">
		select * from tbl_board
	</select>
	
	<insert id="boardInsert" parameterType="com.board.test.domain.Board">
		insert into tbl_board(b_title, b_content)
		values (#{b_title}, #{b_content})
	</insert>
	
	


</mapper>
